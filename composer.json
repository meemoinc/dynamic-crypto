{
    "name": "testinaweb/dynamic-crypto",
    "type": "library",
    "description": "A php library to encrypt and decrypt strings with dynamic keys",
    "keywords": ["encrypt", "encrypter", "decrypt", "decrypter", "mcrypt"],
    "homepage": "https://github.com/testinaweb/dynamic-crypto",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuel Kanah",
            "email": "manuel@kanah.it",
            "homepage": "http://www.labna.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*@dev",
        "behat/behat": "3.0.*@dev"
    },
    "autoload": {
        "psr-4": {"DynamicCrypto\\": "src/"}
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
